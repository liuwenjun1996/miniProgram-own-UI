@import '../components-css.wxss';
.own-search {
  width: 100%;
  /* padding: 10rpx 20rpx; */
  display: flex;
  align-items: center;
  min-height: 60rpx;
}

.own-search-inner {
  width: 100%;
  display: flex;
  align-items: center;
}
.own-input-inner {
  flex: 1;
  display: flex;
  align-items: center;
  position: relative;
}
.own-input {
  border-radius: 10rpx;
  padding-left: 60rpx;
  height: 70rpx;
  font-size: 28rpx;
  flex: 1;
  color: #000;
  margin-right: 20rpx;
  background-color: #f2f2f4;
}
.own-input-icon {
  left: 10rpx;
  top: 12rpx;
  position: absolute;
}
.button {
  border-radius: 10rpx;
}